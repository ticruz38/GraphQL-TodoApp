{
  "name": "graphql-server-boilerplate",
  "version": "1.0.0",
  "description": "Graphql server boilerplate",
  "author": "Fundsquare S.A.",
  "license": "ISC",
  "browseslist": [
    "defaults"
  ],
  "scripts": {
    "start:graph": "nodemon dist/graph.js --watch false",
    "start:graph:prod": "cross-env NODE_ENV=production node dist/graph.js",
    "build:graph": "cross-env NODE_ENV=dev webpack --progress",
    "build:graph:prod": "cross-env NODE_ENV=prod webpack --progress",
    "types": "node_modules/.bin/gql-gen --schema http://localhost:2222/ --template graphql-codegen-typescript-template --out ./graphql-typings.d.ts",
    "test": "node graph/test"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@playlyfe/gql": "^2.6.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.11.1",
    "@types/graphql": "^0.13.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "9.3.0",
    "@types/prop-types": "^15.5.2",
    "apollo-server": "^2.0.0-rc.5",
    "apollo-server-core": "^1.3.6",
    "apollo-server-errors": "^2.0.0-rc.1",
    "apollo-server-express": "^1.3.6",
    "axios": "0.17.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "cross-env": "5.1.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "graphql-cli": "^2.16.4",
    "graphql-code-generator": "^0.10.1",
    "graphql-codegen-typescript-template": "^0.10.2",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "^2.24.0",
    "graphql-type-json": "^0.2.1",
    "jwt-decode": "^2.2.0",
    "log4js": "^2.10.0",
    "moment": "2.18.1",
    "moment-timezone": "^0.5.17",
    "regenerator-runtime": "^0.12.0",
    "subscriptions-transport-ws": "^0.9.8",
    "ts-loader": "3.5.0",
    "typescript": "2.9.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3",
    "webpack-node-externals": "^1.7.2"
  }
}
